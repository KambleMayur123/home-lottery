generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model User {
  id        Int      @id @default(autoincrement())
  name      String
  dob       DateTime
  mobile    String
  address   String
  createdAt DateTime @default(now()) // Ensure this exists in your model
}
