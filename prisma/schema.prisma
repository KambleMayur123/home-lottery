generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model User {
  id        Int      @id @default(autoincrement())
  name      String
  dob       DateTime  // Make sure dob is defined here
  mobile    String   @unique
  address   String
  createdAt DateTime @default(now())
}
